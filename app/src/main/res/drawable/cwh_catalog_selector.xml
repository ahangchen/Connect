<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 按下的状态 -->
    <item android:state_enabled="true" android:state_pressed="true">
        <shape>
            <gradient android:startColor="@color/cwh_white_orange" android:endColor="@color/cwh_white_orange" android:angle="270" />
        </shape>
    </item>
    <item android:state_enabled="true" android:state_checked="true">
        <shape>
            <gradient android:startColor="@color/cwh_white_orange" android:endColor="@color/cwh_white_orange" android:angle="270" />
        </shape>
    </item>
    <!-- 没操作的状态 -->
    <item>
        <shape>
            <gradient android:startColor="#00000000" />
        </shape>
    </item>
</selector>